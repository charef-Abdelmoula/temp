<?xml version="1.0" encoding="utf-8"?>
<odoo>

        <record id="update_record_created_from_view" model="ir.ui.view">
            <field name="name">update.record.created.from.form</field>
            <field name="model">update.record.created.from</field>
            <field name="arch" type="xml">
                <form string="Audit Label">
                    <p class="oe_grey">
                        This wizard will allows you update record created from field on selected records.
                    </p>
                    <group>
                        <field name="record_created_from"/>
                    </group>
                    <footer>
                        <button string="Update" name="update_record_created_from" type="object" default_focus="1" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

</odoo>
