<?xml version="1.0" encoding="utf-8"?>
<odoo>

        <record id="audit_label_view" model="ir.ui.view">
            <field name="name">audit.label.form</field>
            <field name="model">audit.label</field>
            <field name="arch" type="xml">
                <form string="Audit Label">
                    <p class="oe_grey">
                        This wizard will allows you update Audit Label.
                    </p>
                    <group>
                        <field name="audit_label"/>
                    </group>
                    <footer>
                        <button string="Update" name="update_audit_label" type="object" default_focus="1" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

</odoo>
